<head>
  <meta charset="utf-8">
  <link rel="dns-prefetch" href="https://identity.netlify.com">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ renderData.title or title or metadata.title }}</title>
  <meta name="description" content="{{ metaDescription or renderData.metaDescription or summary or metadata.description }}">
    <!-- Open Graph / Facebook -->
  <meta property="og:type" content="website">
  <meta property="og:url" content="{{ metadata.url }}">
  <meta property="og:title" content="{{ metadata.title }}">
  <meta property="og:description" content="{{ metaDescription or renderData.metaDescription or summary or metadata.description }}">
  <meta property="og:image" content="{{ metadata.meta_image }}">

  <!-- Twitter -->
  <meta property="twitter:card" content="summary_large_image">
  <meta property="twitter:url" content="{{ metadata.url }}">
  <meta property="twitter:title" content="{{ metadata.title }}">
  <meta property="twitter:description" content="{{ metaDescription or renderData.metaDescription or summary or metadata.description }}">
  <meta property="twitter:image" content="{{ metadata.meta_image }}">

  <script async src="https://identity.netlify.com/v1/netlify-identity-widget.js"></script>
  {% set css %}
    {% include "assets/css/normalize.css" %}
    {% include "assets/css/variables.css" %}
    {% include "assets/css/base-styles.css" %}
    {% include "assets/css/utilites.css" %}
    {% include "assets/css/site-header.css" %}
    {% include "assets/css/site-footer.css" %}
    {% include "assets/css/blog.css" %}
    {% include "assets/css/projects.css" %}

    {% if page.url == '/' %}
      {% include "assets/css/home-page.css" %}
    {% endif %}

    {% if page.url == '/contact/' %}
      {% include "assets/css/contact.css" %}
    {% endif %}
  {% endset %}
  <style>{{ css | cssmin | safe }}</style>
  {% set js %}
    {% include "assets/js/inline.js" %}
  {% endset %}
  <script>{{ js | jsmin | safe }}</script>
</head>
